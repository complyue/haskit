{
    "publisher": "ComplYue",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/e-wrks/haskit.git"
    },
    "name": "haskit-vscode",
    "displayName": "HaskIt Đ - Haskell Software, Fast Iterations",
    "description": "Quality from Haskell, Speed to Market from Đ (Edh)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:edh"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "edh.SendToEdhTermSession",
                "title": "Edh: Execute Đ Source"
            }
        ],
        "configuration": {
            "title": "HaskIt",
            "properties": {
                "haskit.shell.cmd": {
                    "description": "Đ (Edh) Shell Command",
                    "type": "array",
                    "default": [
                        "hski"
                    ]
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "@types/vscode": "^1.40.0",
        "eslint": "^7.1.0",
        "typescript": "^3.9.5"
    }
}